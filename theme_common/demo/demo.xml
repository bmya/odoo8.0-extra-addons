        <t t-snippet="theme_common.s_color_blocks_2"/>
        <t t-snippet="theme_common.s_color_blocks_4"/>
        <t t-snippet="theme_common.s_column"/>
        <t t-snippet="theme_common.s_discount"/>
        <t t-snippet="theme_common.s_event_list"/>
        <t t-snippet="theme_common.s_icon_box"/>
        <t t-snippet="theme_common.s_icon_box_circle"/>
        <t t-snippet="theme_common.s_icon_box_square"/>
        <t t-snippet="theme_common.s_image_text_2"/>
        <t t-snippet="theme_common.s_lead_bar"/>
        <t t-snippet="theme_common.s_logo_bar"/>
        <t t-snippet="theme_common.s_pricing"/>
        <t t-snippet="theme_common.s_process_step"/>
        <t t-snippet="theme_common.s_profile"/>
        <t t-snippet="theme_common.s_separator_color"/>
        <t t-snippet="theme_common.s_separator_shade"/>
        <t t-snippet="theme_common.s_slide_banner"/>
